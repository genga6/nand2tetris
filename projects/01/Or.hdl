/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
        Nand(a=a, b=a, out=x1);
        Nand(a=b, b=b, out=x2);
        Nand(a=x1, b=x2, out=out);
}